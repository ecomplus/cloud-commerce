{
  "name": "@cloudcommerce/storefront",
  "type": "module",
  "version": "2.10.0",
  "description": "E-Com Plus Cloud Commerce storefront with Astro",
  "bin": {
    "storefront": "./scripts/build-prod.sh"
  },
  "main": "src/index.js",
  "files": [
    "/lib",
    "/types",
    "/*.{js,mjs,ts}",
    "/config/**/*.{json,js,mjs,cjs,ts}",
    "/scripts/**/*.{sh,mjs}",
    "/src",
    "/*.config.cjs",
    "/tsconfig.json",
    "/example.env"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecomplus/cloud-commerce.git",
    "directory": "packages/storefront"
  },
  "author": "E-Com Club Softwares para E-commerce <ti@e-com.club>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecomplus/cloud-commerce/issues"
  },
  "homepage": "https://github.com/ecomplus/cloud-commerce/tree/main/packages/storefront#readme",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev --host",
    "build:ssr": "astro build",
    "build:static": "BUILD_OUTPUT=static astro build",
    "build:prod": "bash scripts/build-prod.sh",
    "preview": "astro preview",
    "astro": "astro",
    "prepare-monorepo": "bash scripts/prepare-monorepo.sh",
    "lint:fix": "eslint -c ../eslint/storefront.staged.eslintrc.cjs src/lib/**/*.{ts,vue,astro} --fix"
  },
  "dependencies": {
    "@astrojs/node": "8.2.5",
    "@astrojs/vue": "4.0.9",
    "@cloudcommerce/api": "workspace:*",
    "@cloudcommerce/config": "workspace:*",
    "@cloudcommerce/i18n": "workspace:*",
    "@ecomplus/utils": "1.5.0-rc.6",
    "@fastify/deepmerge": "^1.3.0",
    "@iconify-json/fa6-brands": "^1.1.18",
    "@iconify-json/heroicons": "^1.1.20",
    "@iconify-json/logos": "^1.1.42",
    "@types/gtag.js": "^0.0.19",
    "@vite-pwa/astro": "^0.3.1",
    "@vueuse/core": "10.9.0",
    "astro": "4.5.12",
    "astro-capo": "^0.0.1",
    "chroma-js": "^2.4.2",
    "dotenv": "^16.4.5",
    "firebase": "^10.9.0",
    "image-size": "^1.1.1",
    "mime": "^3.0.0",
    "mitt": "^3.0.1",
    "semver": "^7.6.0",
    "sharp": "^0.32.6",
    "tailwindcss": "^3.4.1",
    "unocss": "^0.58.6",
    "unplugin-auto-import": "^0.17.5",
    "vite": "^5.2.6",
    "vite-plugin-pwa": "^0.19.7",
    "vue": "^3.4.21",
    "wade": "0.3.3",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@cloudcommerce/eslint": "workspace:*",
    "@cloudcommerce/types": "workspace:*",
    "@types/react": "^18.2.70"
  }
}

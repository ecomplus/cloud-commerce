{
  "name": "@cloudcommerce/storefront",
  "type": "module",
  "version": "0.11.0",
  "description": "E-Com Plus Cloud Commerce storefront with Astro",
  "bin": {
    "storefront": "./scripts/build-prod.sh"
  },
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecomplus/cloud-commerce.git",
    "directory": "packages/storefront"
  },
  "author": "E-Com Club Softwares para E-commerce <ti@e-com.club>",
  "license": "Apache 2.0 with Commons Clause",
  "bugs": {
    "url": "https://github.com/ecomplus/cloud-commerce/issues"
  },
  "homepage": "https://github.com/ecomplus/cloud-commerce/tree/main/packages/storefront#readme",
  "scripts": {
    "dev": "astro dev",
    "dev:cms": "tinacms dev -c \"astro dev\"",
    "start": "tinacms build && astro dev --host",
    "build": "astro build",
    "build:static": "BUILD_OUTPUT=static astro build",
    "build:prod": "bash scripts/build-prod.sh",
    "build:cms": "tinacms build",
    "build:all": "concurrently \"npm run build:prod\" \"npm run build:cms\"",
    "preview": "astro preview",
    "astro": "astro",
    "prepare-monorepo": "bash scripts/prepare-monorepo.sh"
  },
  "dependencies": {
    "@astrojs/image": "0.16.8",
    "@astrojs/node": "5.1.3",
    "@astrojs/vue": "2.1.1",
    "@cloudcommerce/api": "workspace:*",
    "@cloudcommerce/config": "workspace:*",
    "@cloudcommerce/i18n": "workspace:*",
    "@ecomplus/utils": "1.5.0-rc.3",
    "@fastify/deepmerge": "^1.3.0",
    "@headlessui/vue": "^1.7.13",
    "@iconify-json/fa6-brands": "^1.1.11",
    "@iconify-json/heroicons": "^1.1.10",
    "@iconify-json/logos": "^1.1.27",
    "@tinacms/cli": "^1.5.11",
    "@vite-pwa/astro": "^0.0.5",
    "@vueuse/core": "10.1.0",
    "astro": "2.4.5",
    "chroma-js": "^2.4.2",
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3",
    "firebase": "^9.21.0",
    "image-size": "^1.0.2",
    "mime": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semver": "^7.5.0",
    "sharp": "^0.32.1",
    "tailwindcss": "^3.3.2",
    "tinacms": "^1.5.4",
    "unocss": "^0.51.12",
    "vite": "^4.3.5",
    "vite-plugin-pwa": "^0.14.7",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@cloudcommerce/types": "workspace:*"
  }
}
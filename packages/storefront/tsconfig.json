{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@@i18n": ["./node_modules/@cloudcommerce/i18n/src/pt_br.ts"],
      "@@sf/*": ["./src/lib/*"],
      "~/*": ["./src/*"],
      "content/*": ["./content/*"],
      "@@components/*": ["./src/components/*", "./src/lib/components/*"],
    },
    "types": ["vite-plugin-pwa/info"]
  }
}

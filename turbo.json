{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    ".env",
    "tsconfig.json",
    "vite.config.ts"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "**/dist/**",
        "**/lib/**/*.js",
        "**/index.js"
      ]
    },
    "test": {
      "outputs": []
    },
    "test:apps": {
      "outputs": []
    }
  }
}
